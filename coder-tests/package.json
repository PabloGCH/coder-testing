		"start-cluster-profiler": "node --prof dist/server.js -p 4000 -m CLUSTER",
		"forever-fork": "forever start dist/server.js -p 4000",
		"forever-cluster": "forever start dist/server.js -p 4000 -m CLUSTER",
		"pm2-fork": "pm2 start dist/server.js -i 0 -p 4000",
		"pm2-cluster": "pm2 start dist/server.js -i 0 -p 4000 -m CLUSTER",
		"build": "tsc"
